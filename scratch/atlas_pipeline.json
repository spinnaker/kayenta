        "type": "node",
        "scope": "duftler-macbookpro2.roam.corp.google.com",
        "start": "1496436680000",
        "end": "now",
        "step": "PT10S"
      }
    },
    {
      "name": "Fetch Canary from Atlas",
      "refId": "2",
      "requisiteStageRefIds": [

      ],
      "type": "atlasFetch",
      "user": "[anonymous]",
      "atlasQuery": {
        "metricsAccountName":"atlas-acct-1",
        "q": "name,randomValue,:eq",
        "metricSetName": "cpu",
        "type": "node",
        "scope": "duftler-macbookpro2.roam.corp.google.com",
        "start": "1496436680000",
        "end": "now",
        "step": "PT10S"
