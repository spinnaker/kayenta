    {
      "name": "Fetch Canary from Stackdriver",
      "refId": "2",
      "requisiteStageRefIds": [

      ],
      "type": "stackdriverFetch",
      "user": "[anonymous]",
      "stackdriverQuery": {
        "metricsAccountName": "",
        "storageAccountName": "",
        "metricSetName": "cpu",
        "metricType": "compute.googleapis.com/instance/cpu/utilization",
        "scope": "myapp-v010-",
        "intervalStartTimeIso": "2017-05-01T15:13:00Z",
        "intervalEndTimeIso": "2017-05-02T15:27:00Z",
