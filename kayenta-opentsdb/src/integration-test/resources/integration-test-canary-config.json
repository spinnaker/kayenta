{
  "name": "integration-test-canary-config",
  "description": "A simple config for integration testing the Opentsdb metric source Kayenta module.",
  "judge": {
    "judgeConfigurations": {},
    "name": "NetflixACAJudge-v1.0"
  },
  "metrics": [
    {
      "name": "test",
      "query": {
        "metricName": "test.server.request",
        "type": "opentsdb",
        "serviceType": "opentsdb",
        "tags": [
          {
            "key": "response_code",
            "value": "400"
          }
        ],
        "aggregator": "zimsum",
        "rate": true,
        "downsample": "1m-sum"
      },
      "analysisConfigurations": {
        "canary": {
          "direction": "increase"
        }
      },
      "groups": [
        "Integration Test Group"
      ],
      "scopeName": "default"
    }
  ],
  "classifier": {
    "groupWeights": {
      "Integration Test Group": 100
    },
    "scoreThresholds": {
      "marginal": 50,
      "pass": 75
    }
  }
}
